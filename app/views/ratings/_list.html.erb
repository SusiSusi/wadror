<table class="table table-hover table-condensed">

  <thead>
  <tr>
    <th></th>
    <th>Average</th>
  </tr>
  </thead>

  <tbody>
  <% ratings.each do |rating| %>
      <tr>
        <td><%= rating.name %></td>
        <td><%= round(rating.average_rating) %></td>
      </tr>
  <% end %>
  </tbody>

</table>